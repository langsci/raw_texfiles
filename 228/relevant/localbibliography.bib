@Article{mope00,
  author   = {Moore,John and Perlmutter,David M.},
  year     = {2000},
  title    = {What Does It Take to Be a Dative Subject?},
  journal  = {Natural Language \& Linguistic Theory},
  volume   = {18},
  pages    = {373--416},
  number   = {2},
  abstract = {This paper argues that the term "dative subjects" has been applied to two distinct constructions. In one, the surface subject is in the dative case. In the other, a dative-marked nominal that behaves like a subject in certain respects is not, it is claimed here, a surface subject. Dative nominals of the second type have been analyzed in relational grammar as initial subject & final indirect object in the inversion construction (I-nominals). Neither adopting nor rejecting the initial-subject analysis of I-nominals, it is argued only that they are not surface subjects. Russian is shown to have a true dative-subject construction in which surface subjects are in the dative case. Russian also has I-nominals in the dative case which behave like subjects in only two respects; in other respects they fail to behave like subjects. This failure cannot be attributed to their dative case because true dative subjects, also in the dative case, behave like subjects in every respect. It is concluded that dative subjects & I-nominals instantiate distinct constructions which must be recognized as such in syntactic typology & syntactic theory. The implications of this result for the analysis of other languages are discussed. 68 . Adapted from the source document},
  isbn     = {0167-806X, 0167-806X},
  language = {English},
