@Article{baey03,
  author   = {Bar{\dh}dal,J\'ohanna and Eyth{\'o}rsson,Th{\'o}rhallur},
  year     = {2003},
  title    = {Icelandic vs. {G}erman: {O}blique subjects, agreement and expletives},
  journal  = {Papers from the Regional Meetings, Chicago Linguistic Society},
  volume   = {39},
  pages    = {755--773},
  number   = {1},
  abstract = {Traditionally, Icelandic and German oblique subjects have been analyzed as distinct structures with respect to impersonal predicates, dative passives and inverse predicates. In particular, it has been claimed that subject-like obliques in Icelandic behave as syntactic subjects, while in German they behave as syntactic objects. Verb agreement and occurrences with expletives appear to support a non-subject analysis of German obliques. However, with respect to relexivization, conjunction reduction and control infinitives, German obliques behave like syntactic subjects. On this basis, it will be argued that German obliques are indeed syntactic subjects, just as in Icelandic and Faroese. Adapted from the source document},
  isbn     = {0577-7240, 0577-7240},
  language = {English},
  url      = {http://search.proquest.com/docview/868373608?accountid=8330},
}

