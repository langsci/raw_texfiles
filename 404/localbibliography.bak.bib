@inproceedings{wangMandarinDisyllabicWord2022,
  title = {Mandarin {{Disyllabic Word Imitation}} in {{Children}} with and without {{Autism Spectrum Disorder}}},
  booktitle = {Speech {{Prosody}} 2022},
  author = {Wang, Ting and Ding, Heng},
  year = {2022},
