@article{blaauwContributionProsodicBoundary1994,
  title = {The Contribution of Prosodic Boundary Markers to the Perceptual Difference between Read and Spontaneous Speech},
  author = {Blaauw, Eleonora},
  year = {1994},
  month = {sep},
  journal = {Speech Communication},
  volume = {14},
  number = {4},
  pages = {359--375},
  doi = {10.1016/0167-6393(94)90028-0},
  abstract = {Listeners are able to tell apart read-aloud and spontaneously produced speech. Prosody appears to be important for this perceptual distinction. In this paper, the importance of the distribution and realization of prosodic boundaries is investigated. Recordings were made of five male speakers, spontaneously producing so-called instruction monologues. Transcripts of these monologues were read aloud by the same speakers. A perception experiment was carried out to obtain classification scores for isolated utterances selected from the spontaneous and read material. Auditory prosodic transcriptions were made of the entire spontaneous and read monologues, assessing the distribution and realization of underlying prosodic boundaries in both speech types. The underlying prosodic structure was assessed by means of an automatic text-to-speech system. Observed differences in the production of prosodic boundaries in the spontaneous and read material are related to the perceptual classification scores by means of a multiple regression analysis. Results show a significant correlation, suggesting that differences in the distribution and realization of prosodic boundaries contribute significantly to the preceptual differences between spontaneous and read speech. Zusammenfassung Hörer sind in der Lage, zwischen vorgelesener und spontan gesprochener Sprache zu unterscheiden. Die Prosodie spielt dabei anscheinend eine wichtige perzeptive Rolle. In diesem Artikel wird der Beland der Distribution und Realisierung prosodischer Grenzen untersucht. Von fünf männlichen Sprechern wurden Aufnahmen gemacht, die aus spontan produzierten sogennanten Instruktionsmonologen bestanden. TRanskripte dieser spontanen Monologe wurden hergestellt, die von den gleichen Sprechern vorgelesen werden sollten. Ein Perzeptionsexperiment wurde durchgeführt, damit der Prozentsatz von korrekten Entscheidungen für die isolierten Äußerungen, die dem spontan gesprochenen und vorgelesenen Material entnommen worden waren, festgestellt werden konnte. Aufgrund der spontan gesprochenen und vorgelesenen Monologe wurden auditive prosodische Transkriptionen vorgenommen, die die zugrundeliegenden prosodischen Grenzen bezüglich deren Distribution und Realisierung in beiden Sprecharten bestimmen sollten. Die zugrundeliegende prosodische Struktur wurde durch einen Vorleseautomaten ermittelt. Die beobachteten Produktionsunterschiede bezüglich der prosodischen Grenzen zwischen dem spontan gesprochenen und vorgelesenen Material wurden mittels ``multiple regression analysis'' auf den Prozentsatz ``korrekte perzeptive Entscheidungen'' bezogen. Die Ergebnisse zeigen eine signifikante Korrelation, die suggeriert, da\ss{} Unterschiede in der Distribution und Realisierung prosodischer Grenzen signifikant zum perzeptiven Unterschied zwischen spontan gesprochener und vorgelesener Sprache beisteuern. Résumé Il est possible de différencier, à l'écoute, la parole lue et la parole spontanée. La prosodie semble être essentielle pour faire cette distinction. Dans cet article, on étudie l'importance des distributions et réalisations des frontières prosodiques. Des monologues spontanés (appelés monologues ``d'instruction''), émis par cinq locuteurs masculins, ont été enregistrés. Les transcriptions de ces monologues ont été lues à haute voix par les mêmes locuteurs. Une expérience de perception a été menée pour obtenir des scores de classification sur les phrases isolées extraites des énoncés spontanés et lus. Des transcriptions prosodiques de l'ensemble des monologues spontanés et lus ont permis d'évaluer la distribution et la réalisation des frontières prosodiques sous-jacentes dans les deux types de parole. La structure prosodique sous-jacente a été estimée en utilisant un syst{\`m}ee automatique de synthèse de parole. Les différences, entre parole lue et parole spontanée, observées dans la production des frontières prosodiques ont été reliées aux scores de classification perceptive par une analyse de régression multiple. Les résultats montrent une corrélation significative. Ceci suggère que les différences de distribution et de réalisation des frontières prosodiques contribuent de façon significative aux différences perçues entre parole lue et parole spontanée.},
  keywords = {Perceptual distinction,Prosodic boundaries,Read speech,Spontaneous speech},
}

