issn = {14774615},
journal = {Literary and Linguistic Computing},
mendeley-groups = {Dialectometry + logy},
number = {1},
pages = {157--172},
title = {Synchronic patterns of Tuscan phonetic variation and diachronic change: Evidence from a dialectometric study},
volume = {28},
year = {2013}
}


