@misc{Sibler2011,
abstract = {The Syntactic Atlas of German‐speaking Switzerland (Syntaktischer Atlas der Deutschen Schweiz (SADS)) is an atlas project aiming to collect dialect/syntactic data in German speaking Switzerland. So far, only point maps have been generated out of the SADS data basis. Assumptions about the distribution of syntactic phenomena over space do exist but they have never been analysed quantitatively. Selected questions on three syntactic phenomena provide the data framework of this thesis. The goal is on one hand to create areal syntactic dialect maps and on the other to use geostatistical methods to test spatial hypotheses about the distribution of the examined linguistic phenomena. Using relative intensities of syntactic variants at the sampled places the method of Rumpf et al. (2009) has been implemented successfully. It uses Kernel Density Estimation (KDE) to transform linguistic phenomena into areas with dominant variants which are graduated by their intensity of occurence. In the geostatistical analysis part, structural characteristics about compactness, complexity and homogeneity have proven useful to provide an overview of the spatial characteristics of the specific questions. The geostatistical methods to test the spatial hypotheses include Moran's , Getis‐Ord  , Trend Surface Analysis and semivariogram models. The methods provided in this thesis can be extended to further phenomena in the future.},
author = {Sibler, Pius},
booktitle = {Masterarbeit GEO 511},
file = {:C$\backslash$:/Users/P \'{e}ter/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sibler et al. - 2012 - Cartographic Visualization in Support of Dialectology.docx:docx;:C$\backslash$:/Users/P \'{e}ter/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Sibler - 2011 - Visualisierung und Geostatistische Analyse mit Daten des Syntaktischen Atlas der Deutschen Schweiz (SADS).pdf:pdf},
institution = {Universit \"{a}t Z \"{u}rich},
mendeley-groups = {PhD,Dialectometry + logy},
number = {April},
pages = {106},
title = {Visualisierung und Geostatistische Analyse mit Daten des Syntaktischen Atlas der Deutschen Schweiz (SADS): Master's thesis},
type = {Master's thesis},
year = {2011}
}

@inproceedings{Sibler2012,
abstract = {Using data from the Syntactic Atlas of Swiss German Dialects (SADS), several methods for the cartographic visualization of linguistic data are proposed, demonstrated, and evaluated against the requirements of linguistic research. After reviewing the challenges of lin- guistic data visualization, point symbol maps are introduced as a baseline. We then present alter- native visualization methods that present linguistic data in new ways. The first uses Voronoi polygons about the data points to color in the dominant variant per location. The second uses kernel density estimation (KDE) to interpolate intensity values of all variants and thus infer and display the dominant variant per location (incl. at missing value locations). The KDE-based method helps to better see trends in the data and also automatically infer isoglosses. The third new technique uses 3-D visualization to support the exploration of spatial trends, as well as co- occurring variants. As a fourth alternative, measures and methods from geostatistics are used for the visualization of specific, global and local, variations and patterns.},
author = {Sibler, Pius and Weibel, Robert and Glaser, Elvira and Bart, Gabriela},
booktitle = {Proceedings - AutoCarto 2012 - Columbus, Ohio, USA - September 16-18, 2012},
file = {:Users/user/Library/Application Support/Mendeley Desktop/Downloaded/Sibler et al. - 2012 - Cartographic Visualization in Support of Dialectology.pdf:pdf;:Users/user/Library/Application Support/Mendeley Desktop/Downloaded/Sibler et al. - 2012 - Cartographic Visualization in Support of Dialectology.docx:docx},
keywords = {Linguistic area formation,cartographic visualization techniques,dialectology,geostatistics,kernel density estimation},
