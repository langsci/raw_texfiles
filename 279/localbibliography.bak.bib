isbn = {3-484-60406-9},
issn = {0933-1883, 0933-1883},
journal = {Sociolinguistica},
keywords = {*Dialectology (18650),*Europe (23340),*Language Change (41850),*Language Contact (42100),5117: descriptive linguistics,European dialect convergence/divergence,article,dialectology,research review/journal issue introduction},
mendeley-groups = {Dialectometry + logy},
