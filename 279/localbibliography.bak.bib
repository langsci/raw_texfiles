@article{Auer1996,
abstract = {An overview of research on dialect convergence \& divergence is presented, highlighting several issues related to language change in Europe. Concepts of convergence \& divergence are indicated to be used not only in reference to internal factors affecting dialect \& standard varieties of languages, but also in relation to diasystems \& individual speakers' skills in interaction. The orientation of the studies is shown to be a social dialectological one; representations of convergence \& divergence are discussed from a sociolinguistic perspective presenting both structural \& social aspects. The discussion provides a four-part structure to discuss background material, with the first section presenting an overview of several aspects relating to historical linguistics, dialect geography, \& the consequences of language contact. A second section examines aspects related to dialect convergence \& divergence in the context of European languages. Later discussion focuses on theoretical \& methodological issues involving social psychological aspects of accommodation \& variation, with a final section providing commentary on the individual contributions to this issue of Sociolinguistica. 4 Figures, 115 References. R. Meyer},
author = {Auer, Peter and Hinskens, Frans},
doi = {10.1556/AAlim.2015.0002},
