@incollection{Andersen2001,
abstract = {A review of diachronic theories of the past few decades shows that the notion of actualization, defined as "the observable manifestation of grammar innovations in speech," is notable for its absence. To clarify the place of actualization in a theory of grammatical change, innovations are treated as logically consecutive combinations of subchanges that actually overlap in time; such change scenarios include coinage, remedial change, borrowing, extension, transference, \& interference. Each change scenario involves covert subchange processes of adoption \& reanalysis, followed by the overt subchange process of actualization in speech. Typically, speakers who actualize an innovation also produce the older usage under different conditions of appropriateness; the replacement of the latter by the former is characterized as a long-term negotiation process influenced by structural, pragmatic, stylistic, \& social factors. The speaker-oriented theory of evolutive change advocated by Alan Timberlake (1977) is positively evaluated in this context, \& the unidirectionality of grammatical change is attributed to the role of markedness in speakers' preferences for synchronic variants. 42 References. J. Hitchcock},
address = {Amsterdam, Philadelphia},
author = {Andersen, Henning},
booktitle = {Actualization (Current Issues in Linguistic Theory 219)},
doi = {10.1075/cilt.219.11and},
editor = {Andersen, Henning},
file = {:C$\backslash$:/Mendeley PDF/Andersen 2001 {\_}Actualization \_and \_the \_uni \_directio.pdf:pdf},
pages = {225--248},
pmid = {200312380},
publisher = {Benjamins},
title = {Actualization and the (uni)directionality of change},
url = {https://benjamins.com/catalog/cilt.219.11and},
year = {2001}
}

