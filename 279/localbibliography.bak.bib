@article{Labov1963,
abstract = { To summarise: On Martha's Vineyard a small group of fishermen began to exaggerate a tendency already existing in their speech. They did this seemingly subconsciously, in order to establish themselves as an independent social group with superior status to the despised summer visitors. A number of other islanders regarded this group as one which epitomised old virtues and desirable values, and subconsciously imitated the way its members talked. For these people, the new pronunciation was an innovation. As more and more people came to speak in the same way, the innovation gradually became the norm for those living on the island.   Did field work study in 1961 - first study to be done in sociolinguistic framework - move away from question/answer regional dialectology and beginning to know the community to understand variation in lang.He actually recorded people speaking, not just asked them about what they did - revolutionised dialectology. Found that synchonic variation (variation now) is root of diachronic variation (variation over time).  Diphthong PRICE and MOUTH analysed - islanders used more raised centralised onset. But they didn;t use it all the time - was interspeaker variation and intraspeaker variation. Challenged idea of free variation. Coded vowels according to how raised and backed the onset of the diphthong was. Lower onsets received lower score, and more raised and centralised ones received a higher score. Used index  scores to get averages for each speaker and then average for groups of speakers, e.g. women aged 60. (69 speakers).  Results : found that linguistic factors accounted for variation, e.g. following phonetic environment /t, s, p, f/ then centralised variant used                  found non-linguistic factors accounted for variation as well, eg. peoples affiliation/attitudes towards the island.                },
archivePrefix = {arXiv},
arxivId = {arXiv:1011.1669v3},
author = {Labov, William},
doi = {10.1080/00437956.1963.11659799},
