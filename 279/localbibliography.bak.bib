	file = {Full Text PDF:/Users/hanna/Library/Application Support/Zotero/Profiles/s9f2ok4b.default/zotero/storage/FXUCWF7W/McKenzie - 2015 - The sociolinguistics of variety identification and.pdf:application/pdf;Snapshot:/Users/hanna/Library/Application Support/Zotero/Profiles/s9f2ok4b.default/zotero/storage/GSE7S6RV/09658416.2014.html:text/html}
