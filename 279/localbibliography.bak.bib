	pages = {618--628},
	file = {Full Text PDF:/Users/hanna/Library/Application Support/Zotero/Profiles/s9f2ok4b.default/zotero/storage/V5WKSQH2/Noels et al. - 2014 - Ethnolinguistic Orientation and Language Variation.pdf:application/pdf;Snapshot:/Users/hanna/Library/Application Support/Zotero/Profiles/s9f2ok4b.default/zotero/storage/2N3Q6JPS/abstract.html:text/html}
