@article{kulesza_conversational_2016,
	title = {The Conversational Chameleon: {An} {Investigation} {Into} the Link Between Dialogue and Verbal Mimicry},
	volume = {35},
	issn = {0261-927X},
	shorttitle = {The Conversational Chameleon},
	url = {http://journals.sagepub.com/doi/abs/10.1177/0261927X15601460},
	doi = {10.1177/0261927X15601460},
	abstract = {Verbal mimicry research claims that repeating words spoken by another makes people more eager to comply with requests made by the mimicker (e.g., fulfilling a request to donate to charity). Instead, another mechanism might explain these results. Recent studies found that when a request was preceded by engaging a participant in dialogue (defined as a short conversation), the participant was more willing to fulfill the request. Thus, verbal mimicry might be perceived in the same way as dialogue. If this is the case, a theoretical confound would be revealed. To test whether the mechanisms are different of the same, two field studies were conducted using a 2 (dialogue: yes/no) × 2 (mimicry: yes/no) design. The study results revealed two main effects and no interaction effects, which means that verbal mimicry and dialogue are two distinct mechanisms. Interestingly, additive effects for these mechanisms were found.},
	language = {en},
	number = {5},
	urldate = {2017-04-04},
	journal = {Journal of Language and Social Psychology},
	author = {Kulesza, Wojciech and Dolinski, Dariusz and Wicher, Paula and Huisman, Avia},
	year = {2016},
	pages = {515--528},
	file = {SAGE PDF Full Text:/Users/hanna/Library/Application Support/Zotero/Profiles/s9f2ok4b.default/zotero/storage/8XQ72ZNT/Kulesza et al. - 2016 - The Conversational Chameleon An Investigation Int.pdf:application/pdf}
}

@article{mccloy_talker_2015,
