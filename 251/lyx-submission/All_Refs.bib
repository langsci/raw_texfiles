@Article{johansson1973linking,
  author    = {Johansson, Stig},
  title     = {Linking and intrusive /r/ in English: A case for a more concrete phonology},
  journal   = {Studia Linguistica},
  year      = {1973},
  volume    = {27},
  number    = {1-2},
  pages     = {53--68},
  abstract  = {A phonological description of intrusive /r/ in English. Johansson describes dialects in which the r sound only surfaces within words before vowels, and at the ends of words with inflectional or derivational endings beginning with vowels. Words like "stir" that surface with a final r in other dialects of English are analyzed as vowel-final in this dialect, such that a rule of insertion is required to account for the r that occurs between the end of the word and the beginning of the vowel-initial suffix. r also surfaces across word-boundaries in the same phonological environment, in what Johansson describes as the same "rhythmic group". A distinction is made between this scenario: "linking r", and instances of apparent innovation: "intrusive r", which occurs after specific vowels. The latter cases do not have a known historic source nor any counterpart in the spelling. Of these, the most common environment is after schwa across a word boundary in British Received Pronunciation (RP). An argument is put forward for treating the two phenomena as aspects of the same phonological rule. Various arguments are also presented for selecting an insertion rather than a deletion analysis for linking/intrusive r. 

Johansson describes the historic origins as a classic instance of rule inversion: historic deletion re-analzyed at some stage by speakers as insertion.  This rule inversion is described as a simplification of the synchronic system. He claims also that the re-analysis was facilitated by the phonetic similarity of r and schwa in EnglishHe assumes that historic r loss occurred in the first place due to the fact that preceding vowel assimilation made the final r redundant in terms of word dismabiguation.},
  publisher = {Wiley Online Library},
}

@Article{broadbent1991linking,
  author   = {Broadbent, Judith},
  title    = {Linking and intrusive r in English},
  year     = {1991},
  pages    = {281--302},
  abstract = {A treatment of linking/intrusive r in the non-rhotic dialect of West Yorkshire. This dialect is chosen due to the fact that no socially motivated 'suppression' of intrusive r occurs, as it does in RP, due to conscious efforts to conform with the spelling of the words. Broadbent also offers a unified account of linking and intrusive r: as glide formation, and thus as the same phenomenon as intervocalic j and w, an optional process recruited by speakers in order to break up vowel sequences. In RP j is described as surfacing after high front vowels, w after high back vowels, and r after non-high vowels. However, in WY it is necessary to describe the contexts as non-low front tense vowels for j; non-low tense back vowels for w, and non-high lax vowels for r. Broadbent uses this fact to argue for spreading of vocalic features into an empty onset position as the underlying process for all three cases.

Provides a review of other proposals.},
  journal  = {University College London Working Papers in Linguistics 3},
}

