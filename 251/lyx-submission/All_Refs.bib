  author    = {Zoll, Cheryl},
  title     = {Ghost segments and optimality},
  booktitle = {The Proceedings of the West Coast Conference on Formal Linguistics},
  year      = {1993},
  volume    = {12},
  pages     = {183--199},
  abstract  = {Zoll analyzes ghost segments as behaving analogously to floating features and tones that require a `docking' site in order to surface. She argues that a skeletal tier is not necessary to account for the phenomenon, as earlier accounts have hypothesized, and that moraic structure is sufficient.  In regular epenthesis, a syllable position, root node and default features are all added. In ghost segment epenthesis, the features are already present and only need to attach to the newly generated syllable and root node.},
}

@Article{archangeli1991syllabification,
  author    = {Archangeli, Diana},
  title     = {Syllabification and prosodic templates in Yawelmani},
  year      = {1991},
  volume    = {9},
  number    = {2},
  pages     = {231--283},
  abstract  = {Archangeli proposes that the ghost consonants that surface with certain suffixes when attaching to roots of a certain form fall out of her general analysis of templatic morphology in Yawelmani. Suffixes with ghost segments in initial position belong to the class of suffixes that impose skeletal templates on the roots to which they attach. But only the first two consonants and vowel of the root map directly to the template, the remaining properties of the word are explained by Yawelmani's right-to-left syllabification algorithm. Unsyllabified consonants trigger epenthesis, and `extra' consonants at the edge of the word simply syllabify. Ghost segments are defined as lacking a syllabic specification underlyingly: they cannot project a prosodic node. However, they are available to satisfy templatic slots when other consonants are not available. Thus, for example, in biconsonantal roots where a slot already exists for them to dock to ghost segments surface in coda position of the second required template syllable.},
  journal   = {Natural Language \& Linguistic Theory},
  publisher = {Springer},
}

@Article{zygis2010typology,
  author   = {{\.Z}ygis, Marzena},
  title    = {Typology of consonantal insertions},
  year     = {2010},
  volume   = {52},
  pages    = {111--140},
  abstract = {This work contains brief descriptions of around 70 languages (many of which are discussed in other papers cited in this bibliography). Zygis, like Vaux, proposes a typology of consonant insertion without reference to markedness. The scope of what she terms epenthesis is broader, however, including insertion motivated either by i) grammatical considerations, (ii) phonetic considerations, or (iii) prosodic. She uses grammatical to refer to insertions conditioned by any of phonology, morphology, syntax, or some combination; phonetics includes articulation, acoustics, aerodynamics and perception; and prosodic refers to insertions that are restricted to specific prosodic boundaries or domains. She claims that coronal segments are the most likely in grammatical insertion, and coronals and glottals in prosodic insertion, but offers no explanation for why this should be the case.},
  journal  = {ZAS Papers in Linguistics},
}

@PhdThesis{levin1985metrical,
