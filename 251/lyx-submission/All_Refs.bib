@Article{ortmann1998consonant,
  author   = {Ortmann, Albert},
  title    = {Consonant epenthesis: its distribution and phonological specification},
  year     = {1998},
  pages    = {51--76},
  abstract = {Predicts possible epenthetic consonants in hiatus. Proposes a universal restriction on segments involved in `systematic' epenthesis (default). r in British English (feature spreading, following Broadbent, but r is not a glide; it just has a vocalic node under the C place node...); conditioned by vowel context (never after high vowels). 
 n in HIgh German: clicit environments: default epenthesis; other environments, floating segment as in Dutch (because not generalized, not completely phonological.
default consonant epenthesis can arise through sonorant codas that have been lost - rule inversion. Inserted consonants are `simple' phonological = minimally specified; no more than one underlying feature specification/node (consonantl and sonorant are already attached to root node so you get them for free). epenthetic segments 'universally unmarked' with respect to other consonants. Assumes attested segments: glottal stop, t, n, l, r, w, j. Language-specific markedness though, but this might work out as minimal specification given the entire language inventory...: l less marked than n in Baka. n as less marked than t in German, Dutch; but t as less marked in A. Campa.r is only specified as [-high]. Assumes coronals are less marked than other palces of articulation.

Analyzes rule inversion from deletion to epenthesis as emergence of the unmarked??
Axininca Campa
minimalist feature specification},
  journal  = {Phonology and Morphology of the Germanic Languages. T{\"u}bingen, Niemeyer},
}

@Article{mccarthy1993case,
  author   = {McCarthy, John J},
  title    = {A case of surface constraint violation},
  year     = {1993},
  volume   = {38},
  issue    = {2},
  pages    = {169-195},
  abstract = {Boston intrusive r. Only occurs after monophongal vowels that are legal word-finally, meaning that r only occurs morpheme-finally, or word final. The high vowels are analyzed as dipthongal, with the offglide resyllabifying across the boundary, forestalling r intrusion. Analyzes etymological r deletion as resulting utterance finally and pre-consonantally. Both r deletion and r insertion are analyzed as productive synchronic processes, motivating an OT analysis. r-deletion is driven by a constraint against r in post-nuclear position. r-insertion is driven by a constraint against short vowels at the ends of prosodic words.},
  journal  = {Constraint-based theories in multilinear phonology, special issue of Canadian Journal of Linguistics},
}

