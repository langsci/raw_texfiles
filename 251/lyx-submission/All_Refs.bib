@Article{rubach2000glide,
  author    = {Rubach, Jerzy},
  title     = {Glide and glottal stop insertion in Slavic languages: A DOT analysis},
  year      = {2000},
  volume    = {31},
  number    = {2},
  pages     = {271--317},
  abstract  = {Epenthesis data from 6 Slavic dialects. In Colloquial Slovak [j] epenthesis is characterized as intervocalic spreading, and achieved with a markedness constraint against constricted glottis (glottal stop), and one against projecting a root node. In Bulgarian word-initial glottal stop insertion is accounted for by a faithfulness constraint enforcing continguity (insertion at edges only), and one against multiply linked elements (no glides with features copied from neighboring vowels). Czech, which has both word-initial glottal stop insertion and intervocalic glide insertion cannot be captured by a consistent ranking of the proposed set of constraints. THe author uses this to argue for introducing derivational levels to OT. At the first level glides can be inserted and glottal stops banned; on the second, glottal stop can be inserted and glides banned.},
