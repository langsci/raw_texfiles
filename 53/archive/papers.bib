	Keywords = {robotics,vision,planning,pomdp},
	Title = {{HiPPo}: {H}ierarchical {POMDP}s for {P}lanning {I}nformation {P}rocessing and {S}ensing {A}ctions on a {R}obot},
	Url = {http://www.cognitivesystems.org/publications/sridharanetal08hippo.pdf},
