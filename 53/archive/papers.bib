	Journal = {Artificial Intelligence Review},
	Keywords = {language,evolution of language},
	Number = {3-4},
	Pages = {229--256},
	Title = {The emergence of embodied communication in artificial agents and humans},
	Volume = {24},
	Year = {2008},
