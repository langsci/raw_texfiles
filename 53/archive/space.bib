	Editor = {Freksa, C. and Brauer, W. and Habel, C. and Wender, K. F.},
	Pages = {263---286},
	Publisher = {Springer},
	Title = {{Spatial knowledge representation for human-robot interaction}},
	Year = {2003},
	Bdsk-File-1 = {}}

@article{moratz2009cognitive,
	Abstract = {The question addressed in this paper is which types of spatial reference human users employ in the interaction with a robot and how a cognitively adequate model of these strategies can be implemented. In experiments we explore how human users approach an artificial communication partner, which was designed to mimic spatial reference among humans. Our findings show that spatial reference in human-robot interaction differs from natural situations in human-human interaction in several resects. For instance, many users unexpectedly employed fine-grained, path-based, instructions rather than specifying the intended goal object of the action directly. If instructions were not successful, participants created less and less complex descriptions. Those users who did specify the goal object were found to employ those kinds of spatial reference strategies implemented in our computational model. In particular, they exploited the presence of several similar objects by perceiving and referring to the, linguistically as a group.},
	Annote = {aim: human robot interaction in action (drive to a specific location/object)

experimental results:
* ONLY half the participants used goal object description strategy [fahre zum linke wuerfel]
* other strategies 1) path [1 meter geradaus, dann 2 meter links...] 2) action [drehe das hintere rad]
* group-based reference occurs often
* speakers adapt by reducing linguistic complexity (simpler lexicon, less morphology, use of infinitives
* speakers almost exclusively used robot's perspective (also expressively asked for the robot's front)

total 183 instructions:
* 102/183 group based reference
* 69/102 used particular expression scheme
* 19/43 cardbox was used as reference object

quotes:
"Thus, the issue at hand is, 'Which of these similar objects do you refer to?'. This perspective triggers ways of linguistic reference hitherto largely ignored in the literature on spatial reference systems" (pp 3)

},
	Author = {Moratz, R. and Fischer, K. and Tenbrink, T.},
	Date-Added = {2010-10-14 22:48:40 +0100},
	Date-Modified = {2013-01-14 08:00:05 +0000},
	Journal = {International Journal on Artificial Intelligence Tools},
	Number = {4},
	Pages = {589--611},
	Title = {Cognitive modeling of spatial reference for human-robot interaction},
	Volume = {10},
	Year = {2009},
	Bdsk-File-1 = {}}

