@article{Inkelas&Zoll:2007,
  title={Is grammar dependence real? {A} comparison between cophonological and indexed constraint approaches to morphologically conditioned phonology},
  author={Inkelas, Sharon and Zoll, Cheryl},
  journal = {Linguistics},
  volume = {45.1},
  pages = {133-171},
  year={2007},
  annote = {Two major proposals have been put forth to deal with morphologically conditioned phonology: indexed constraints and cophonologies. Inkelas and Zoll compare the two, arguing in favor of cophonologies based on descriptive and explanatory grounds. While cophonologies assumes a set of general constraints that can be reranked within distinct phonological systems of a language, indexed constraint theory assumes a single constraint ranking for a language, with constraints that can be specified to apply only to certain morphemes or categories. We need cophonologies to account for free variation within languages anyway, and cophonologies are more economical than are indexed constraints. Additionally, cophonologies make predictions about stem scope and locality which indexed constraint theory does not make, but which hold cross-linguistically.}
}

