@article{Marchese:ACAL,
	author = {Lynell Zogbo},
	title = {Central vowels in the {K}ru language family: Innovation and areal spreading},
	journal = {ACAL 47 Proceedings},
	annote = {Here Zogbo proposes that central vowels have recently been innovated in and are presently spreading throughout Eastern Kru. While Godié and Bakwe each have four or five phonemic central vowels (13-15 total underlying vowels), Bété and other Eastern Kru languages have at most two central vowels. In Western Kru there is at most one. Interestingly, the Mande languages spoken closest to Eastern Kru, specifically Dan, have three or more central vowels as well, but are not spoken contiguously to those Kru languages with more than three central vowels. Zogbo proposes that this suggests population movement, where southern Eastern Kru languages like Godié used to be spoken further north, closer to Mande, and what are now northern Eastern Kru languages moved in to separate Mande from Godi'e and Bakwe.},
	pages = {725--750},
