@article{Yu:2004,
  title={Explaining final obstruent voicing in {L}ezgian: phonetics and history},
  author={Alan Yu},
  journal={Language},
  pages={73--97},
  year={2004},
  publisher={JSTOR},
  annote = {Yu starts by citing Steriade (1993) who claims that phonological contrast is maintained in perceptually salient contexts but neutralized in environments with poor perceptual cues (Licensing by Cue (LBC) (Steriade 1997). Yu addresses the fact that LBC forces neutralization toward voicelessness with data from Lezgian, where underlyingly voiceless segments surface as voiced in a number of environments. The seemingly unnatural synchronic patter in Lezgian arose from what Yu calls ``a series of natural sound changes''. He reminds phonologists that synchronic patterns are often the result of complex diachronic history and that synchronic patterns reflect that history and are not always phonetically predictable.}
}

@article{Wedel:2006,
  title={Exemplar models, evolution and language change},
  author={Wedel, Andrew B},
  journal={The linguistic review},
  volume={23},
  number={3},
  pages={247--274},
  year={2006},
  annote = {The goal of this paper is to show that exemplar-based models of languages are convenient for modeling language evolution parallel to Darwinian natural selection. Wedel discusses how within a given population of language learners, linguistic categories evolve via genetic drift, blending inheritance, and natural selection (though the second is not actually attested). While evolutionary pressures lead to change, lexical access processes select against ambiguity, influencing (lack of) sound change.}
}

@inproceedings{Wedel:2004,
  title={Category competition drives contrast maintenance within an exemplar-based production/perception loop},
  author={Wedel, Andrew},
  booktitle={Proceedings of the 7th meeting of the acl special interest group in computational phonology: Current themes in computational phonology and morphology},
  pages={1--10},
  year={2004},
  organization={Association for Computational Linguistics},
  annote = {Wedel argues for an emergent rather than stipulated view of dispersion, with a specific interest in contrast within categories. Making his argument via models rather than natural language data, Wedel shows that between-category competition for form variants supports contrast maintenance of form-category pairs, explained best by a model where ``phonological but not morphological categories are coextensive with motor units'' (p.9). With respect to language change, Wedel predicts a lack of sound change when it would result in loss of paradigmatic contrast. This is exactly where we find `exceptions' to sound change generally.}
}


