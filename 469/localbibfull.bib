@article{HallD:2011,
  title={Phonological contrast and its phonetic enhancement: Dispersedness without dispersion},
  author={Hall, Daniel Currie},
  journal={Phonology},
  volume={28},
  number={01},
  pages={1--54},
  year={2011},
  publisher={Cambridge Univ Press},
  annote = {In order to account for dispersion in vocalic and consonantal inventories, Hall proposes a phonological theory where only contrastive features are specified (cf. Dresher 2009), and those features are phonetically enhanced. This approach differs from the Dispersion Theory of Liljencrants and Lindblom (1972) where vowels repel each other for maximal dispersion and constraint-based approaches like Flemming (2002) and Padgett (2003) where constraints on contrast determine optimal languages, rather than optimal forms. Instead, only contrastive featuers are encoded, and redundant features react phonetically to maximize contrast. This paper contains a thorough description of previous versions (constraint-based and not) of Dispersion Theory, and argues for a novel approach.}
}

@article{Liljencrants:1972,
