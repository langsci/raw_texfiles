@incollection{Deal:2013,
  title={Outwards-sensitive phonologically-conditioned allomorphy in Nez Perce},
  author={Deal, Amy Rose and Wolf, Matthew},
  booktitle={The Morphosyntax-Phonology Connection},
  editor = {Vera Gribanova and Stephanie Shih},
  year={2016},
  annote={This paper attempts to address the question of when morphemes are realized (or vocabulary items are inserted) relative to each other. Deal and Wolf argue that it cannot be `all at once' like in parallel OT. They provide evidence from Nez Perce that inside-out seriality as previously suggested in Distributed Morphology, and, to a degree, Stratal OT, is not strict. Instead, there are cyclic domains of spell-out, and within a domain morphemes can be inserted in a non-inside-out fashion (parallel or outside-in). Deal and Wolf assume three key features of a DM-like approach: cyclicity (but not necessarily at every instance of syntactic merge), monotonicity (insertion adds information), and linearity (conditioning environments for a morpheme must be adjacent to that morpheme). Deal and Wolf show evidence for a morphological domain above the syntactic Asp head. A similar analysis works in Stratal OT.}
}

